        }
      ]
    },
    {
      "name": "Kill Switch - Test",
      "url": "{{baseUrl}}/admin/services/test-service/kill",
      "method": "POST",
      "sortNum": 40001,
      "containerId": "Admin",
      "headers": [
        {
          "name": "Authorization",
          "value": "Bearer {{authToken}}"
        },
        {
          "name": "Content-Type",
          "value": "application/json"
        }
      ],
      "body": {
        "type": "json",
        "raw": "{\n  \"reason\": \"Test kill switch\",\n  \"dry_run\": true\n}"
      },
      "tests": [
        {
          "type": "res-code",
          "custom": "",
          "action": "equal",
          "value": "200"
        }
      ]
    },
    {
      "name": "Audit Log",
      "url": "{{baseUrl}}/admin/audit",
      "method": "GET",
      "sortNum": 40002,
      "containerId": "Admin",
      "headers": [
        {
          "name": "Authorization",
          "value": "Bearer {{authToken}}"
        }
      ],
      "tests": [
        {
          "type": "res-code",
          "custom": "",
          "action": "equal",
