  "requests": [
    {
      "name": "Health Check",
      "url": "{{baseUrl}}/health",
      "method": "GET",
      "sortNum": 10000,
      "containerId": "Health",
      "tests": [
        {
          "type": "res-code",
          "custom": "",
          "action": "equal",
          "value": "200"
        }
      ]
    },
    {
      "name": "Login",
      "url": "{{baseUrl}}/auth/login",
      "method": "POST",
      "sortNum": 20000,
      "containerId": "Auth",
      "headers": [
        {
          "name": "Content-Type",
          "value": "application/json"
        }
      ],
      "body": {
        "type": "json",
        "raw": "{\n  \"email\": \"{{testEmail}}\",\n  \"password\": \"{{testPassword}}\"\n}"
      },
      "tests": [
        {
          "type": "res-code",
          "custom": "",
          "action": "equal",
          "value": "200"
        },
        {
          "type": "set-env-var",
          "custom": "authToken",
          "action": "setto",
          "value": "json.accessToken"
        }
      ]
    },
    {
      "name": "Get Current User",
      "url": "{{baseUrl}}/auth/me",
      "method": "GET",
      "sortNum": 20001,
      "containerId": "Auth",
      "headers": [
        {
          "name": "Authorization",
          "value": "Bearer {{authToken}}"
        }
      ],
      "tests": [
        {
          "type": "res-code",
          "custom": "",
          "action": "equal",
          "value": "200"
        }
      ]
    },
    {
      "name": "List Tenants",
      "url": "{{baseUrl}}/tenants",
      "method": "GET",
      "sortNum": 30000,
      "containerId": "Tenants",
      "headers": [
        {
          "name": "Authorization",
          "value": "Bearer {{authToken}}"
        }
      ],
      "tests": [
        {
          "type": "res-code",
          "custom": "",
          "action": "equal",
          "value": "200"
        }
      ]
    },
    {
      "name": "List Leads",
      "url": "{{baseUrl}}/leads",
      "method": "GET",
      "sortNum": 40000,
      "containerId": "Leads",
      "headers": [
        {
          "name": "Authorization",
          "value": "Bearer {{authToken}}"
        },
        {
          "name": "x-tenant-id",
          "value": "{{tenantId}}"
        }
      ],
      "tests": [
        {
          "type": "res-code",
          "custom": "",
          "action": "equal",
          "value": "200"
        }
      ]
    },
    {
      "name": "Create Lead",
      "url": "{{baseUrl}}/leads",
      "method": "POST",
      "sortNum": 40001,
      "containerId": "Leads",
      "headers": [
        {
          "name": "Authorization",
          "value": "Bearer {{authToken}}"
        },
        {
          "name": "x-tenant-id",
          "value": "{{tenantId}}"
        },
        {
          "name": "Content-Type",
          "value": "application/json"
        }
      ],
      "body": {
        "type": "json",
        "raw": "{\n  \"firstName\": \"Test\",\n  \"lastName\": \"Lead\",\n  \"email\": \"test@example.com\",\n  \"phone\": \"555-0100\"\n}"
      },
      "tests": [
        {
          "type": "res-code",
          "custom": "",
          "action": "equal",
          "value": "201"
        },
        {
          "type": "set-env-var",
          "custom": "leadId",
          "action": "setto",
          "value": "json.id"
        }
      ]
    },
    {
      "name": "Get Calendar Events",
      "url": "{{baseUrl}}/calendar/events",
      "method": "GET",
      "sortNum": 50000,
